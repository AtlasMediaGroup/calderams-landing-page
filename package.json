{
  "name": "landing-page",
  "homepage": "https://calderams.com",
  "version": "0.1.0",
  "license": "Atlas Media Group Ltd",
  "private": true,
  "authors": [
  "Ryan Wild <ryan.wild@atlas-media.co.uk>"
  "Matt Kent <matt.kent@calderams.com>",
  "Mark Robertson <mark.robertson@atlan-event.com>",
  "Alex Harris <alex.harris@atlas-media.co.uk>"
  ],
  "dependencies": {
    "gatsby-plugin-glamor": "^2.0.7",
    "glamor": "^2.20.40",
    "jsdom": "^13.1.0",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-entypo": "^1.3.0",
    "react-entypo-icons": "^1.4.1",
    "react-helmet": "^5.2.0",
    "react-particles-js": "^2.4.2",
    "react-scripts": "2.1.3",
    "react-tweet-embed": "^1.1.1",
    "typeface-roboto": "0.0.54",
    "typography": "^0.16.18",
    "typography-breakpoint-constants": "^0.16.18",
    "typography-plugin-code": "^0.16.18",
    "unfetch": "^4.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "npm run test && react-scripts build",
    "test": "react-scripts test --coverage --env=jsdom",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "jest-canvas-mock": "^1.1.1-beta.1",
    "react-test-renderer": "^16.7.0",
    "sinon": "^7.2.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "back-end/**/*.{js,jsx}",
      "!src/index.js",
      "!src/serviceWorker.js"
    ],
    "coverageReporters": [
      "text",
      "html"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "standard": {
    "globals": [
      "fetch"
    ],
    "env": "jest",
    "rules": {
      "react/jsx-indent": "off"
    }
  }
}
